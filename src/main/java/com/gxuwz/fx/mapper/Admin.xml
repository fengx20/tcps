<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
   
    <mapper namespace="com.gxuwz.fx.mapper.AdminMapper">
    
       <select id="adminlogin" parameterType="String" resultType="boolean">
       		select ifnull((select admin from admin where admin = #{0} and password = #{1}),false)
       </select>    
       
       
    
    </mapper>